    {
        "id": "94e7eef20cfd0676",
        "type": "ui_template",
        "z": "tab_mixer_kpi",
        "group": "ui_grp_kpi",
        "name": "KPI Bar",
        "order": 17,
        "width": 6,
        "height": 6,
        "format": "<!-- KPI Bars (vertical slim bars) -->\n<div class=\"kpi-bars\">\n  <div class=\"bars\">\n    <div class=\"bar\" ng-repeat=\"b in bars\">\n      <div class=\"track\">\n        <div class=\"fill\" ng-style=\"{'height': b.pct + '%', 'background': b.color}\"></div>\n      </div>\n      <div class=\"value\">{{b.value}}{{b.unit ? (' ' + b.unit) : ''}}</div>\n      <div class=\"label\">{{b.label}}</div>\n    </div>\n  </div>\n\n  <div class=\"cls-badge\" ng-if=\"classification\">\n    {{classification}}\n  </div>\n</div>\n\n<style>\n  .kpi-bars {\n    padding: 0px 4px 0px;  /* ← 上下 padding 缩小 */\n    margin: 0;             /* ← 避免额外外边距 */\n}\n.kpi-bars .bars{\n    display:flex; \n    gap:8px;               /* ← bar 与 bar 之间更靠近 */\n    justify-content:space-around; \n    align-items:flex-end;\n    margin: 0;             /* ← 清除默认 margin */\n    padding: 0;            /* ← 清除默认 padding */\n}\n  .kpi-bars .bar{\n    width: 48px; text-align:center; color:#eee; font-size:10px;\n  }\n  .kpi-bars .track{\n    width: 10px;             /* 柱条更瘦 */\n    height: 150px;           /* 柱条高度，可调大一点更秀气 */\n    margin: 0 auto 6px;\n    background:#2a2a2a; border-radius:6px; position:relative; overflow:hidden;\n    box-shadow: inset 0 0 0 2px rgba(255,255,255,0.06);\n  }\n  .kpi-bars .fill{\n    position:absolute; bottom:0; left:0; right:0;\n    border-radius:6px;\n    transition: height .35s ease;\n  }\n  .kpi-bars .value{\n    font-weight:700; margin-bottom:2px; white-space:nowrap;\n    font-size:10px;\n  }\n  .kpi-bars .label{\n    line-height:1.1; font-size:10px;\n  }\n  .kpi-bars .cls-badge{\n    margin-top:10px; display:inline-block; padding:4px 10px; border-radius:999px;\n    background:#374151; color:#fff; font-weight:700; letter-spacing:.2px;\n  }\n</style>\n\n<script>\n(function(scope){\n  scope.bars = [];\n  scope.classification = \"\";\n\n  scope.$watch('msg', function(m){\n    if(!m || !m.payload) return;\n\n    // 接收 fn 的第二口 {payload:{bars:[], classification:''}}\n    if (m.payload.bars && Array.isArray(m.payload.bars)){\n      scope.bars = m.payload.bars.map(function(it){\n        var min = Number(it.min)||0, max = Number(it.max)||1;\n        var v = Number(it.value)||0;\n        var pct = ((v - min) / (max - min)) * 100;\n        pct = Math.max(0, Math.min(100, pct));\n        return {\n          label: it.label,\n          value: it.value,\n          unit:  it.unit || \"\",\n          color: it.color || \"#49a6ff\",\n          pct:   pct\n        };\n      });\n    }\n    if (typeof m.payload.classification === 'string'){\n      scope.classification = m.payload.classification;\n    }\n  });\n})(scope);\n</script>\n",

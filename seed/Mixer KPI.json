        "func": "// 清空 flow 中的输入缓存\n['N5','N7','N9','N13','N14'].forEach(k => flow.set(k, undefined));\n\n// Propeller Qty 恢复默认 1\nlet s = flow.get('settings') || { N11:1, N12:415, rho:1000, kPF:0.9, pumpK:0.67 };\ns.N11 = 1;\nflow.set('settings', s);\n\n// 发给输入框 → 空字符串，清掉界面值\nconst msgClearInputs = { payload: \"\" };\n\n// 发给结果 → 空对象，清掉文字\nconst msgClearResult = { payload: {} };\n\n// 发给 KPI Bar → 空数组，清掉柱条\nconst msgClearBars = { payload: { bars: [], classification: \"\" } };\n\n// 输出 1: 给输入框\n// 输出 2: 给 result + bar（用数组一次性发两条）\nreturn [ msgClearInputs, [ msgClearResult, msgClearBars ] ];\n",
